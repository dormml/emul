<project name="v9t9.build" default="main" basedir=".">

	<!--
	This is a target build.  It is driven mostly by the common.xml
	file (and eclipse.xml, git.xml, etc. below that).  In this file,
	just point to the target build and let the common script do the work.
	-->

	<property name="base.dir" value="/tmp/launchers-build" />
	<property name="base.git.url" value="/home/ejs/devel" />
	<property name="eclipse.home" value="/usr/local/install/eclipse-3.8-really/eclipse" />
	<property name="7z.executable.location" value="/usr/bin/7z" />
	<property name="baseos" value="win32" />
	<property name="basews" value="win32" />
	<property name="basearch" value="x86" />
	<property name="configs" value="linux,gtk,x86_64 &amp; linux,gtk,x86 &amp; 
		win32,win32,x86 &amp; win32,win32,x86_64 &amp;
		macosx,cocoa,x86 &amp; macosx,cocoa,x86_64" />
	
	<property name="archiveName" value="v9t9.zip"/>
		
	<property name="allRepos" value="emul" />
	
	<!-- Establish target-specific properties. -->
	<property file="build.properties" />

	<!-- common scripts location -->
	<property name="common.scripts" value="${basedir}/../../common" />

	<!-- define our top-level build directory (containing build.xml) -->
	<property name="target.build.name" value="launchers" />
	<property name="target.product.name" value="v9t9" />

	<!-- get the common build steps -->
	<import file="${common.scripts}/common.xml" />


	<!-- define our features and plugins filesets -->

	<resources id="target.fileset.plugins">
		<fileset id="@{baseId}.plugins" dir="@{dir}/@{pluginBase}">
			<include name="${base.src.dir}/emul/v9t9/v9t9-java/v9t9-rcp/**" />
		</fileset>
	</resources>
	
	<resources id="target.fileset.features">
	</resources>

	<!-- test features -->
	<resources id="target.fileset.plugins.test">
	</resources>
	<resources id="target.fileset.features.test">
	</resources>
	<property name="target.test.features" value="" />
	
	
</project>

