	</resources>

	<resources os="Mac OS X" arch="amd64">
		<jar href="libs/org.eclipse.swt.cocoa.macosx.x86_64.jar" />
		<nativelib href="v9t9j-natives-macosx-intel.jar" download="eager"/>
	</resources>

	<resources os="Mac OS X" arch="x86">
		<jar href="libs/org.eclipse.swt.cocoa.macosx.x86.jar" />
		<nativelib href="v9t9j-natives-macosx-intel.jar" download="eager"/>
	</resources>

	-->
	
	<application-desc 
		main-class="v9t9.gui.Emulator">
		<!--
		<argument>-C -c /tmp/v9t9</argument>	
		-->
    </application-desc>
    
</jnlp>
