all-local:	$(OUTS)

clean-local:
	rm -f $(OUTS)

install-data-local:
	mkdir -p $(pkgdatadir)/forth/roms $(pkgdatadir)/forth/site-forth
	for i in $(OUTS) ; do \
		$(INSTALL_DATA) $$i $(pkgdatadir)/forth/roms/$$i ; \
	done
	for i in $(FORTHCOMMON) ; do \
		$(INSTALL_DATA) $$i $(pkgdatadir)/forth/$(notdir $$i) ; \
	done
	$(INSTALL_DATA) nforth.cnf $(pkgdatadir)/nforth.cnf

force:

$(ROMADDR): romaddr.c
	@CC@ -o $@ $^

$(TIASM):
	( cd ../TIAsm && ant  )

#$(TIASM):
#	make -C $(dir $(TIASM))

nforth0.rom: $(FORCE) $(ROM99) $(TIASM)
	$(TIASM_RUN) -DENHANCED_MEMORY nforth0.tsm -e nforth0.rom -l nforth0.lst 

nforthA.rom: $(FORCE) $(ROM99) $(TIASM)
	$(TIASM_RUN) -DENHANCED_MEMORY nforth.tsm -e nforthA.rom -l nforthc.lst 

nforth.grm: nforth.gsm $(TIASM)
	$(TIASM_RUN) -DENHANCED_MEMORY $< -g $@ -l nforthg.lst

@HAVE_GFORTH_TRUE@nforth.prm nforthc.bin nforthg.bin nforthc.sym nforthg.sym: $(FORCE) $(TIASM) $(ROMADDR) nforth0.rom $(FORTH99) $(FORTHCOMMON)
@HAVE_GFORTH_TRUE@	rm -f $(LIST)
@HAVE_GFORTH_TRUE@	-@GFORTH@ -e ": get-rom-addr $(shell $(ROMADDR) nforth0.rom) ; create mach-file ,\" `pwd`/99config.fs" 99build.fs -e bye &>out.lst 
@HAVE_GFORTH_TRUE@	if test -f $(LIST) ; then \
@HAVE_GFORTH_TRUE@		if ! grep 'Nothing' < $(LIST) &>/dev/null; then tail $(LIST); false; fi ; \
@HAVE_GFORTH_TRUE@	fi
@HAVE_GFORTH_TRUE@	python getforthsyms.py <$(LIST) >nforthc.sym
@HAVE_GFORTH_TRUE@	cp -f nforthc.sym nforthg.sym

@HAVE_GFORTH_TRUE@nforthB.rom nforthB.sym: $(FORCE) nforth.prm nforth0.rom $(ROMADDR)
@HAVE_GFORTH_TRUE@	cp -f nforth0.rom nforthB.rom && \
@HAVE_GFORTH_TRUE@	SIZE=$(shell $(ROMADDR) nforth0.rom) && \
@HAVE_GFORTH_TRUE@	if [ ! -z "$$SIZE" ] ; then \
@HAVE_GFORTH_TRUE@		dd if=nforth.prm of=nforthB.rom bs=1 seek=$$SIZE count=$$[16384-$$SIZE]; \
@HAVE_GFORTH_TRUE@	fi ;		 
@HAVE_GFORTH_TRUE@	python getforthsyms.py <$(LIST) >nforthB.sym

@HAVE_GFORTH_FALSE@nforth.prm nforthc.bin nforthg.bin: $(FORCE) $(TIASM) $(ROMADDR) nforth0.rom $(FORTH99) $(FORTHCOMMON)
@HAVE_GFORTH_FALSE@	@echo "GNU forth (gforth) is not installed, kernel cannot be rebuilt."

